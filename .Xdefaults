#include ".config/xcolors/mine"
!#include ".config/xcolors/cinderwick"
!#incldue ".config/xcolors/sicpsnake2"
!#include ".config/xcolors/unexist"
!#include ".config/xcolors/tango"
!#include ".config/xcolors/digerati"
!#include ".config/xcolors/jasonwryan"
!#include ".config/xcolors/zenburn"
!#include ".config/xcolors/phraktured"


! Font
urxvt*font:		-windows-proggycleanszbp-medium-r-normal--13-80-96-96-c-70-iso8859-1
urxvt*boldFont:		-windows-proggycleanszbp-medium-r-normal--13-80-96-96-c-70-iso8859-1


! Options
urxvt*geometry:		80x25
urxvt*transparent:	true
urxvt*tintColor:	white
urxvt*shading:		40
urxvt*fading:		20
urxvt*saveLines:	32000
urxvt*visualBell:	false
urxvt*scrollBar:	false
urxvt*scrollstyle:	plain
urxvt*TtyOutput:	false
urxvt*TtyKeyPress:	true
urxvt*TtyBuffer:	true
urxvt*loginShell:	true
urxvt*urlLauncher:	/usr/bin/firefox
urxvt*cutchars:		"()*,<>[]{}|'
urxvt*perl-lib:		/usr/lib/urxvt/perl
urxvt*perl-ext-common:	default,matcher
urxvt*mapAlert:		true
urxvt*termName:		rxvt
urxvt*.matcher.button:	1
urxvt*utmpInhibit:	true
urxvt*print-pipe:	cat > $(echo urxvt.dump.`date +'%Y%M%d%H%m%S'`)
urxvt*cursorColor:	#ffcc00

! lal clock
lal*font:       dejavu sans mono
lal*fontsize:   12
lal*bold:       false
lal*color:      white
lal*width:      50
lal*format:     %k:%M
