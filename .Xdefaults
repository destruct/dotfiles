! .Xdefaults
! Author Contact: destruct.c{at}gmx{dot}net

! Colors
*background:  #222222
*foreground: white
! Black
*color0:      #000000
*color8:      #555753
! Red
!*color1:      #ff6565
*color1: #f12e2e
*color9:      #ff8d8d
! Green
*color2:      #93d44f
*color10:     #c8e7a8
! Yellow
*color3:      #eab93d
*color11:     #ffc123
! Blue
*color4:      #204a87
*color12:     #3465a4
! Magenta
*color5:      #ce5c00
*color13:     #f57900
! Cyan 
*color6:      #89b6e2
*color14:     #46a4ff
! White
*color7:      #cccccc
*color15:     #ffffff

! Font
urxvt*font:		-windows-proggycleanszbp-medium-r-normal--13-80-96-96-c-70-iso8859-1

! Options
urxvt*geometry:		80x25
urxvt*transparent:	true
urxvt*tintColor:	white
urxvt*shading:		40
urxvt*fading:		20
urxvt*saveLines:	32000
urxvt*visualBell:	false
urxvt*scrollBar:	false
urxvt*scrollstyle:	plain
urxvt*TtyOutput:	false
urxvt*TtyKeyPress:	true
urxvt*TtyBuffer:	true
urxvt*loginShell:	true
urxvt*urlLauncher:	/usr/bin/firefox
urxvt*cutchars:		"()*,<>[]{}|'
urxvt*perl-lib:		/usr/lib/urxvt/perl
urxvt*perl-ext-common:	default,matcher
urxvt*mapAlert:		true
urxvt*termName:		rxvt
urxvt*.matcher.button:	1
urxvt*utmpInhibit:	true
urxvt*print-pipe:	cat > $(echo urxvt.dump.`date +'%Y%M%d%H%m%S'`)
urxvt*cursorColor:	#ffcc00

! lal clock
lal*font:       dejavu sans mono
lal*fontsize:   12
lal*bold:       false
lal*color:      white
lal*width:      50
lal*format:     %k:%M
